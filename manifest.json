{
    "name": "Pockets",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Organize your tabs in a flash!",
    "options_page": "options.html",
    "permissions": ["storage", "tabs"],
    "browser_action": {
        "default_icon": "icons8-lightning-bolt-64.png"
    },
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
    }
  }